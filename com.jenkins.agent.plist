<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.jenkins.agent</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/java</string>
      <string>-jar</string>
      <string>/Users/jenkins/Jenkins-agent/agent.jar</string>
      <string>-url</string>
      <string>https://jenkins.fptmedicare.vn/</string>
      <string>-secret</string>
      <string>@/Users/jenkins/Jenkins-agent/secret-file</string>
      <string>-name</string>
      <string>Agent-Mac</string>
      <string>-webSocket</string>
      <string>-workDir</string>
      <string>/Users/jenkins/Jenkins-agent</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
      <key>NetworkState</key>
      <true/>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/jenkins/Jenkins-agent/agent.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/jenkins/Jenkins-agent/agent-error.log</string>
  </dict>
</plist>
